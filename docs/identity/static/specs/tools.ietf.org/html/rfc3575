   34       Event-Response               [<a href="./rfc2882" title="&quot;Network Access Servers Requirements: Extended RADIUS Practices&quot;">RFC2882</a>]
   40       Disconnect-Request           [<a href="#ref-DynAuth" title="&quot;Dynamic Authorization Extensions to Remote Authentication Dial In User Service (RADIUS)&quot;">DynAuth</a>]
   41       Disconnect-ACK               [<a href="#ref-DynAuth" title="&quot;Dynamic Authorization Extensions to Remote Authentication Dial In User Service (RADIUS)&quot;">DynAuth</a>]
   42       Disconnect-NAK               [<a href="#ref-DynAuth" title="&quot;Dynamic Authorization Extensions to Remote Authentication Dial In User Service (RADIUS)&quot;">DynAuth</a>]
   43       CoA-Request                  [<a href="#ref-DynAuth" title="&quot;Dynamic Authorization Extensions to Remote Authentication Dial In User Service (RADIUS)&quot;">DynAuth</a>]
   44       CoA-ACK                      [<a href="#ref-DynAuth" title="&quot;Dynamic Authorization Extensions to Remote Authentication Dial In User Service (RADIUS)&quot;">DynAuth</a>]
   45       CoA-NAK                      [<a href="#ref-DynAuth" title="&quot;Dynamic Authorization Extensions to Remote Authentication Dial In User Service (RADIUS)&quot;">DynAuth</a>]
   50       IP-Address-Allocate          [<a href="./rfc2882" title="&quot;Network Access Servers Requirements: Extended RADIUS Practices&quot;">RFC2882</a>]
   51       IP-Address-Release           [<a href="./rfc2882" title="&quot;Network Access Servers Requirements: Extended RADIUS Practices&quot;">RFC2882</a>]
   250-253  Experimental Use
   254      Reserved
   255      Reserved                     [<a href="./rfc2865" title="&quot;Remote Authentication Dial In User Service (RADIUS)&quot;">RFC2865</a>]

































