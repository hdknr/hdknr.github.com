seqdiag {
  default_fontsize = 16;

  End-User  -> Client [label = "Identifier"];
  Client -> Client [label = "Normalize\nIdentifer" ];
  Client <-- Client [label = "(principal,host )" ] ; 
  Client  -> host [ label = "SWD(\n port,\n principal,\n service\n )" ,note = "(=OP)"];
  host => host [ label="solve(\n principal,\nservice\n)" ,return="location" ];
  Client <-- host [ label = "location" ] ;
  End-User <-- Client [label = "location(302)" ] ;  
  End-User -> OP [ label="authz request" ];
}
